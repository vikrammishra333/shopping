<!DOCTYPE html>
<html>
<head>
  <title>MFShop</title>
  <%= render 'layouts/stylesheets' %>
  <%= render 'layouts/javascript' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id="main-body">
    <div id="header">
     <%= render 'layouts/header' %>
    </div>
    <div style="clear:both"></div>
    <div id="content">
       <%= yield %>
    </div>
    <div style="clear:both"></div>
    <hr class="footer-separator" />
    <div id="footer"
      <%= render 'layouts/footer' %>
    </div>
  </div>
</body>
</html>
